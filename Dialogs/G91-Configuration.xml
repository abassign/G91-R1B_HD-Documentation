            <slider>
                <row>0</row>
                <col>6</col>
                <name>hydraulic_pressure_loss</name>
                <property>fdm/jsbsim/systems/hydraulics/lost-performance-system</property>
                <min>0.1</min>
                <max>1.0</max>
                <step>0.01</step>  
                <binding>
                    <command>dialog-apply</command>
                </binding>
            </slider>
            <text>
                <row>0</row>
                <col>7</col>
                <label>9999</label>
                <live>true</live>
                <format>%2.2f</format>
                <property>fdm/jsbsim/systems/hydraulics/lost-performance-system</property>
                <color>
                    <red>0.9</red>
                    <green>0.9</green>
                    <blue>0.0</blue>
                </color>
            </text>
        </group>
    </group>
    
