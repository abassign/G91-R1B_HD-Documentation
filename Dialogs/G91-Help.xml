<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <name>G91_Help_dialog</name>
    <layout>vbox</layout>
    <default-padding>7</default-padding>
    <modal>false</modal>
    <width>500</width>
    
    <!-- title bar -->
    <group>
        <layout>hbox</layout>
        <empty><stretch>true</stretch></empty>
        <text>
            <label>Help G91</label>
        </text>
        <empty><stretch>true</stretch></empty>
        <button>
            <legend/>
            <key>Esc</key>
            <pref-width>16</pref-width>
            <pref-height>16</pref-height>
            <border>2</border>
            <binding>
                <command>dialog-close</command>
            </binding>
        </button>
    </group>
    
    <hrule/>
    
    <group>
        <layout>hbox</layout>
        <group>
            <layout>hbox</layout>
            <halign>left</halign>
            <text>
                <label>C (upercase)</label>
                <halign>left</halign>
                <row>0</row>
                <col>0</col>
            </text>
            <text>
                <label> - </label>
                <halign>left</halign>
                <row>0</row>
                <col>1</col>
            </text>
            <text>
                <label>Canopy open/close</label>
                <halign>left</halign>
                <row>0</row>
                <col>2</col>
            </text>
            <text>
                <label>F5</label>
                <halign>left</halign>
                <row>1</row>
                <col>0</col>
            </text>
            <text>
                <label> - </label>
                <halign>left</halign>
                <row>1</row>
                <col>1</col>
            </text>
            <text>
                <label>Drag chute activate</label>
                <halign>left</halign>
                <row>1</row>
                <col>2</col>
            </text>
        </group>
    </group> 

<PropertyList>
