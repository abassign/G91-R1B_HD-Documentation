        <fcs_function>
            <function>
                <ifthen>
                    <gt>
                        <abs>
                            <difference>
                                <p>systems/gauges/PHI/wind/digit1A</p>
                                <p>systems/gauges/PHI/wind/digit1AW</p>
                            </difference>
                        </abs>
                        <v>0.1</v>
                    </gt>
                    <p>systems/gauges/PHI/wind/digit-inc</p>
                    <v>0.0</v>
                </ifthen>
            </function>
            <output>systems/gauges/PHI/wind/digit1AW-inc</output>
        </fcs_function>
        
        <fcs_function>
            <function>
                <ifthen>
                    <gt>
                        <p>systems/gauges/PHI/wind/digit1AW-inc</p>
                        <v>0.0</v>
                    </gt>
                    <ifthen>
                        <gt>
                            <p>systems/gauges/PHI/wind/digit1AW</p>
                            <v>9.9</v>
                        </gt>
                        <v>0.0</v>
                        <sum>
                            <p>systems/gauges/PHI/wind/digit1AW</p>
                            <p>systems/gauges/PHI/wind/digit1AW-inc</p>
                        </sum>
                    </ifthen>
                    <ifthen>
                        <lt>
                            <p>systems/gauges/PHI/wind/digit1AW</p>
                            <v>0.0</v>
                        </lt>
                        <v>9.9</v>
                        <sum>
                            <p>systems/gauges/PHI/wind/digit1AW</p>
                            <p>systems/gauges/PHI/wind/digit1AW-inc</p>
                        </sum>
                    </ifthen>
                </ifthen>
            </function>
